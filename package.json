{
  "name": "vue-sailor-icons",
  "version": "1.0.0",
  "description": "Nice open source icons as Vue functional components.",
  "repository": {
    "url": "sailoricons/vue-sailor-icons",
    "type": "git"
  },
  "main": "dist/vue-sailor-icons.cjs.js",
  "cdn": "dist/vue-sailor-icons.min.js",
  "unpkg": "dist/vue-sailor-icons.min.js",
  "module": "dist/vue-sailor-icons.es.js",
  "files": [
    "dist",
    "icons"
  ],
  "sideEffects": false,
  "scripts": {
    "test": "echo 'no tests!'",
    "prepublishOnly": "npm run build",
    "build": "npm run build:icons && npm run build:dist",
    "build:dist": "bili --format umd,es,cjs,umd-min --js buble --name vue-sailor-icons",
    "build:icons": "node build && babel src/components -d icons",
    "example": "poi --serve --config example/poi.config.js",
    "build:example": "poi --prod --config example/poi.config.js",
    "prebuild:example": "npm run build:icons"
  },
  "author": "katalonne <catalinbesleagadev@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-helper-vue-jsx-merge-props": "^2.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "bili": "^5.0.5",
    "cherio": "1.0.0-rc.2",
    "eslint-config-rem": "^4.0.0",
    "fs-extra": "^8.0.1",
    "jsdom": "15.1.1",
    "lodash.kebabcase": "^4.1.1",
    "marked": "^0.6.0",
    "pascal-case": "^2.0.1",
    "poi": "^12.4.4",
    "prismjs": "^1.6.0",
    "sailor-icons": "1.0.0",
    "typeface-nunito": "^0.0.72",
    "v-tippy": "^1.0.0",
    "vue": "^2.5.22",
    "vue-github-badge": "^1.0.1",
    "vue-template-compiler": "^2.5.22"
  },
  "babel": {
    "presets": [
      [
        "poi/babel",
        {
          "jsx": "vue"
        }
      ]
    ]
  }
}
